<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="iOS test suite on iPhone Simulator">

    <parameter name="OS" value="Android"/>
    <parameter name="appType" value="Phone"/>
    <parameter name="deviceType" value="Emulator"/>
    <parameter name="deviceName" value="Nexus 5"/>
    <parameter name="version" value="9"/>
    <parameter name="appName" value="ebay.apk"/>
    <test name="Go to Home page" preserve-order="true">
        <classes>
            <class name="Nexus5.TestHomePage"/>
        </classes>
    </test>
</suite>
